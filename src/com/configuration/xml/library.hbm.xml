<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.bean.LibraryBean" table="Library">
    <id column="ID" name="id">
      <generator class="increment"/>
    </id>
    <property column="StudentID" name="studentID"/>
    <property column="BookID" name="bookID"/>
    <property column="BookName" name="bookName"/>
    <property column="IssueDate" name="issueDate"/>
    <property column="SubmitDate" name="submitDate"/>
    <property column="Fine" name="fine"/>
  </class>
</hibernate-mapping>
